{
  "name": "applicaster-oauth",
  "version": "1.0.0",
  "description": "this modules is a express.js middle to integrate with Applicaster's oauth authentication system accounts.applicaster.com",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/nodemon src/index.js --exec node_modules/.bin/babel-node",
    "build": "node_modules/.bin/babel src -d lib",
    "prepublish": "npm run build && cp src/auth.html lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applicaster/applicaster-oauth-js.git"
  },
  "keywords": [
    "applicaster",
    "oauth",
    "npm"
  ],
  "author": "Applicaster",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/applicaster/applicaster-oauth-js/issues"
  },
  "homepage": "https://github.com/applicaster/applicaster-oauth-js#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.9.1"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-polyfill": "^6.7.4"
  }
}
